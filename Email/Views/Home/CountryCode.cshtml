    <script src="~/Views/Home/js/intlTelInput.min.js"></script>
    <script src="~/Views/Home/js/utils.js"></script>
    <script>
        $('#phone').intlTelInput();
    </script>
    <form>
        @Html.Editor("ContactNumber", new { htmlAttributes = new { @class = "form-control" } })
