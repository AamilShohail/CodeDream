    <h1>International Telephone Input</h1>
    <script src="~/intl-tel-input-master/build/js/intlTelInput.js"></script>
    <script src="~/Views/Home/js/intlTelInput.js"></script>
    <script src="~/Views/Home/js/intlTelInput-jquery.js"></script>
